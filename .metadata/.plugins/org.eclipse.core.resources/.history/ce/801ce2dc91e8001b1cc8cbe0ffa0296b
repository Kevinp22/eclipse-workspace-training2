package com.neosoft.basics;

import java.util.Scanner;

public class Program2 {

	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int row=sc.nextInt();
		for(int i =0;i<row;i++) {
			System.out.println(" ");
		}
		for(int j=0; j<=i;j++) {
			System.out.println(j);
		}

	}

}
